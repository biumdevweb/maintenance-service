<!-- This is a placeholder for the PNG file. In a real implementation, this would be a binary PNG file. -->
<!-- For now, we'll create the SVG version that can be converted to PNG -->
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 72 72">
  <!-- Background con gradiente -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E3A8A" />
      <stop offset="100%" style="stop-color:#0ea5e9" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#EA580C" />
      <stop offset="100%" style="stop-color:#f97316" />
    </linearGradient>
  </defs>
  
  <!-- Background rounded -->
  <rect width="72" height="72" rx="12" fill="url(#bgGradient)"/>
  
  <!-- Icona principale: shield con gear -->
  <g transform="translate(36, 36)">
    <!-- Shield esterno -->
    <path d="M0 -16 L-14 -8 L-14 6 C-14 18 0 28 0 28 C0 28 14 18 14 6 L14 -8 Z" 
          fill="white" opacity="0.95"/>
    
    <!-- Gear inside shield -->
    <path d="M0 -5 L1.2 -4.4 L1.2 -3.2 L2.2 -2.5 L2.2 -1.3 L1.2 -0.6 L1.2 0.6 L0 1.2 L-1.2 0.6 L-1.2 -0.6 L-2.2 -1.3 L-2.2 -2.5 L-1.2 -3.2 L-1.2 -4.4 Z" 
          fill="url(#iconGradient)"/>
    
    <!-- Center circle -->
    <circle cx="0" cy="0" r="1.8" fill="white"/>
    
    <!-- Check mark -->
    <path d="M-1.2 0 L-0.5 0.8 L1.2 -1" 
          stroke="#1E3A8A" stroke-width="0.5" fill="none" stroke-linecap="round"/>
  </g>
</svg>