<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 96 96">
  <!-- Background con gradiente -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E3A8A" />
      <stop offset="100%" style="stop-color:#0ea5e9" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#EA580C" />
      <stop offset="100%" style="stop-color:#f97316" />
    </linearGradient>
  </defs>
  
  <!-- Background rounded -->
  <rect width="96" height="96" rx="16" fill="url(#bgGradient)"/>
  
  <!-- Icona principale: shield con gear -->
  <g transform="translate(48, 48)">
    <!-- Shield esterno -->
    <path d="M0 -22 L-19 -11 L-19 8 C-19 24 0 37 0 37 C0 37 19 24 19 8 L19 -11 Z" 
          fill="white" opacity="0.95"/>
    
    <!-- Gear inside shield -->
    <path d="M0 -7 L1.6 -6.1 L1.6 -4.3 L2.9 -3.3 L2.9 -1.7 L1.6 -0.8 L1.6 0.8 L0 1.6 L-1.6 0.8 L-1.6 -0.8 L-2.9 -1.7 L-2.9 -3.3 L-1.6 -4.3 L-1.6 -6.1 Z" 
          fill="url(#iconGradient)"/>
    
    <!-- Center circle -->
    <circle cx="0" cy="0" r="2.4" fill="white"/>
    
    <!-- Check mark -->
    <path d="M-1.6 0 L-0.6 1 L1.6 -1.3" 
          stroke="#1E3A8A" stroke-width="0.6" fill="none" stroke-linecap="round"/>
  </g>
</svg>